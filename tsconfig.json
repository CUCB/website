{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "paths": {
      "*": ["./src/@customTypes/*"],
      "$lib": ["./src/lib"],
      "$lib/*": ["./src/lib/*"]
    },
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "types": ["vitest/importMeta", "vite-plugin-sentry/client"]
  }
}
