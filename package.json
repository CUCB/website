{
  "name": "cucb-website",
  "description": "The (PHP-free) website for the Cambridge University Ceilidh Band",
  "version": "0.0.1",
  "scripts": {
    "prepare": "husky install",
    "check": "svelte-check && tsc --noEmit",
    "dev": "vite dev --port 3000 --host 0.0.0.0",
    "build": "vite build",
    "start": "vite preview",
    "start:ci": "export NODE_ENV=test; node build",
    "build_start": "vite build; node ./build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "test:unit": "vitest",
    "format": "pretty-quick",
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose stop",
    "docker:down": "docker-compose down",
    "docker:test": "docker-compose -f docker-compose.test.yml up --exit-code-from cypress",
    "svelte-check": "svelte-check",
    "orm": "env NODE_OPTIONS='--loader ts-node/esm' npx mikro-orm",
    "resetdb": "npm run orm -- migration:fresh && npm run orm -- seeder:run"
  },
  "watch": {
    "dev": {
      "patterns": [
        "tsconfig.json",
        "rollup.config.js",
        "package.json"
      ]
    },
    "build_start": {
      "patterns": [
        "tsconfig.json",
        "rollup.config.js",
        "package.json"
      ]
    }
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.15.1",
    "@mikro-orm/cli": "^5.6.8",
    "@mikro-orm/core": "^5.6.8",
    "@mikro-orm/migrations": "^5.6.8",
    "@mikro-orm/postgresql": "^5.6.8",
    "@mikro-orm/seeder": "^5.6.8",
    "@sentry/browser": "^7.28.1",
    "@sentry/node": "^7.28.1",
    "@sentry/svelte": "^7.28.1",
    "@sentry/tracing": "^7.28.1",
    "@silvia-odwyer/photon": "^0.3.11",
    "@sveltejs/adapter-node": "^1.0.0",
    "@sveltejs/kit": "^1.0.0",
    "@sveltejs/package": "^1.0.1",
    "@types/google.maps": "^3.51.1",
    "bcrypt": "^5.0.1",
    "body-scroll-lock": "^3.1.5",
    "cookie": "^0.4.1",
    "cookie-signature": "^1.1.0",
    "cypress-plugin-tab": "^1.0.5",
    "dotenv": "^10.0.0",
    "emailjs": "^3.4.0",
    "fuse.js": "^6.4.6",
    "html-escaper": "^3.0.3",
    "husky": "^6.0.0",
    "ical-generator": "^3.1.1",
    "immutable": "^4.0.0-rc.12",
    "isomorphic-fetch": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "luxon": "^1.26.0",
    "mailparser": "^3.2.0",
    "node-fetch": "^3.1.0",
    "photon-web": "^0.3.2",
    "runtypes": "^6.3.1",
    "svelte": "^3.54.0",
    "svelte-color-picker": "^1.0.7",
    "svelte-easy-crop": "^1.0.5",
    "tippy.js": "^6.3.1",
    "typescript": "^4.3.4"
  },
  "devDependencies": {
    "@mikro-orm/entity-generator": "^5.6.8",
    "@percy/cli": "^1.13.0",
    "@percy/cypress": "^3.1.0",
    "@rollup/plugin-wasm": "^6.0.1",
    "@sentry/vite-plugin": "^0.3.0",
    "@types/bcrypt": "^5.0.0",
    "@types/body-scroll-lock": "^2.6.1",
    "@types/compression": "^1.7.0",
    "@types/cookie-signature": "^1.1.0",
    "@types/express-session": "^1.17.3",
    "@types/html-escaper": "^3.0.0",
    "@types/http-proxy": "^1.17.6",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/jsonwebtoken": "^8.5.2",
    "@types/luxon": "^1.27.0",
    "@types/mailparser": "^3.0.2",
    "@types/mocha": "^10.0.1",
    "@types/node-fetch": "^2.5.10",
    "@types/polka": "^0.5.2",
    "@types/sinon": "^10.0.13",
    "autoprefixer": "^10.2.6",
    "chai-sorted": "^0.2.0",
    "cypress": "^12.0.0",
    "ical.js": "^1.5.0",
    "mdsvex": "^0.9.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.5",
    "prettier": "^2.3.1",
    "prettier-plugin-svelte": "^2.3.1",
    "pretty-quick": "^3.1.1",
    "sass": "^1.35.1",
    "sinon": "^15.0.1",
    "svelte-check": "^2.1.0",
    "svelte-preprocess": "^4.7.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.3.0",
    "vite": "^4.0.0",
    "vitest": "^0.26.2"
  },
  "engines": {
    "node": ">= 12.17.0"
  },
  "type": "module",
  "resolutions": {},
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./mikro-orm.config.ts"
    ]
  }
}
