{
  "name": "cucb-website",
  "description": "The (PHP-free) website for the Cambridge University Ceilidh Band",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev -p 3000 --host 0.0.0.0",
    "build": "svelte-kit build",
    "start": "svelte-kit preview",
    "start:ci": "export NODE_ENV=test; svelte-kit preview -p 4000 --host 0.0.0.0",
    "test:ci:ff": "run-p --race start:ci cy:run:ci:ff",
    "test:ci:chrome": "run-p --race start:ci cy:run:ci:chrome",
    "cy:run:ci:chrome": "wait-for-it localhost:4000 -t 60 -- percy exec -- cypress run --record --key $CYPRESS_KEY --browser chrome",
    "cy:run:ci:ff": "wait-for-it localhost:4000 -t 60 -- percy exec -- cypress run --record --key $CYPRESS_KEY --browser firefox",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "format": "pretty-quick",
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose stop",
    "docker:down": "docker-compose down",
    "docker:test": "docker-compose -f docker-compose.test.yml up --exit-code-from cypress",
    "hasura:console": "cd hasura && hasura console --endpoint \"http://localhost:8080\" --admin-secret myadminsecretkey",
    "watch:dev": "npm-watch dev",
    "svelte-check": "svelte-check"
  },
  "watch": {
    "dev": {
      "patterns": [
        "tsconfig.json",
        "rollup.config.js",
        "package.json"
      ]
    }
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-scroll-lock": "^3.1.5",
    "cookie": "^0.4.0",
    "cookie-signature": "^1.0.6",
    "dotenv": "^8.2.0",
    "emailjs": "^3.4.0",
    "escape-html": "^1.0.3",
    "fuse.js": "^6.4.6",
    "graphql": "^15.5.0",
    "graphql-tag": "^2.12.4",
    "http-proxy": "^1.18.1",
    "immutable": "^4.0.0-rc.12",
    "jsonwebtoken": "^8.5.1",
    "luxon": "^1.26.0",
    "mailparser": "^3.2.0",
    "md5": "^2.3.0",
    "runtypes": "^6.3.0",
    "svelte-color-picker": "^1.0.7",
    "tippy.js": "^6.3.1",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@percy/cli": "^1.0.0-beta.48",
    "@percy/cypress": "^3.1.0",
    "@sveltejs/adapter-node": "^1.0.0-next.17",
    "@sveltejs/kit": "^1.0.0-next.95",
    "@types/bcrypt": "^3.0.1",
    "@types/body-scroll-lock": "^2.6.1",
    "@types/compression": "^1.7.0",
    "@types/escape-html": "^1.0.1",
    "@types/express-session": "^1.17.3",
    "@types/http-proxy": "^1.17.5",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/luxon": "^1.26.5",
    "@types/mailparser": "^3.0.2",
    "@types/md5": "^2.3.0",
    "@types/node-fetch": "^2.5.10",
    "@types/polka": "^0.5.2",
    "autoprefixer": "^10.2.5",
    "chai-sorted": "^0.2.0",
    "cypress": "^7.2.0",
    "cypress-pipe": "^2.0.0",
    "hasura-cli": "^1.3.3",
    "husky": "^6.0.0",
    "mdsvex": "^0.9.3",
    "node-wait-for-it": "^0.2.0",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.9.0",
    "postcss": "^8.2.13",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "pretty-quick": "^3.1.0",
    "sass": "^1.32.12",
    "svelte": "=3.38.0",
    "svelte-check": "1.5.2",
    "svelte-preprocess": "^4.7.2",
    "tslib": "^1.14.1",
    "vite": "^2.2.4"
  },
  "engines": {
    "node": ">= 12.17.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "type": "module",
  "resolutions": {}
}
