{
  "name": "cucb-website",
  "description": "The (PHP-free) website for the Cambridge University Ceilidh Band",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev -p 3000 --host 0.0.0.0",
    "build": "svelte-kit build",
    "start": "svelte-kit preview",
    "start:ci": "export NODE_ENV=test; node build",
    "test:ci:ff": "run-p --race start:ci cy:run:ci:ff",
    "test:ci:chrome": "run-p --race start:ci cy:run:ci:chrome",
    "cy:run:ci:chrome": "wait-for-it localhost:3000 -t 60 -- percy exec -- cypress run --record --key $CYPRESS_KEY --browser chrome",
    "cy:run:ci:ff": "wait-for-it localhost:3000 -t 60 -- percy exec -- cypress run --record --key $CYPRESS_KEY --browser firefox",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "format": "pretty-quick",
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose stop",
    "docker:down": "docker-compose down",
    "docker:test": "docker-compose -f docker-compose.test.yml up --exit-code-from cypress",
    "hasura:console": "cd hasura && hasura console --endpoint \"http://localhost:8080\" --admin-secret myadminsecretkey",
    "watch:dev": "npm-watch dev",
    "watch:build": "npm-watch build_start",
    "build_start2": "svelte-kit dev --host 0.0.0.0",
    "build_start": "svelte-kit build; node ./build",
    "svelte-check": "svelte-check",
    "prepare": "husky install"
  },
  "watch": {
    "dev": {
      "patterns": [
        "tsconfig.json",
        "rollup.config.js",
        "package.json"
      ]
    },
    "build_start": {
      "patterns": [
        "tsconfig.json",
        "rollup.config.js",
        "package.json"
      ]
    }
  },
  "dependencies": {
    "@sveltejs/adapter-node": "^1.0.0-next.56",
    "@sveltejs/kit": "^1.0.0-next.203",
    "bcrypt": "^5.0.1",
    "body-scroll-lock": "^3.1.5",
    "cookie": "^0.4.1",
    "cookie-signature": "^1.1.0",
    "cypress-plugin-tab": "^1.0.5",
    "dotenv": "^10.0.0",
    "emailjs": "^3.4.0",
    "escape-html": "^1.0.3",
    "fuse.js": "^6.4.6",
    "graphql": "^15.5.0",
    "graphql-tag": "^2.12.4",
    "http-proxy": "^1.18.1",
    "husky": "^6.0.0",
    "ical-generator": "^3.1.1",
    "immutable": "^4.0.0-rc.12",
    "isomorphic-fetch": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "luxon": "^1.26.0",
    "mailparser": "^3.2.0",
    "md5": "^2.3.0",
    "node-fetch": "^3.1.0",
    "photon-web": "^0.3.2",
    "runtypes": "^6.3.1",
    "svelte": "3.44.0",
    "svelte-color-picker": "^1.0.7",
    "svelte-easy-crop": "^1.0.5",
    "tippy.js": "^6.3.1",
    "trace-unhandled": "^2.0.1",
    "typescript": "^4.3.4",
    "vite-plugin-wasm-pack": "^0.1.9"
  },
  "devDependencies": {
    "@percy/cli": "^1.0.0-beta.52",
    "@percy/cypress": "^3.1.0",
    "@types/bcrypt": "^5.0.0",
    "@types/body-scroll-lock": "^2.6.1",
    "@types/compression": "^1.7.0",
    "@types/escape-html": "^1.0.1",
    "@types/express-session": "^1.17.3",
    "@types/http-proxy": "^1.17.6",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/jsonwebtoken": "^8.5.2",
    "@types/luxon": "^1.27.0",
    "@types/mailparser": "^3.0.2",
    "@types/md5": "^2.3.0",
    "@types/node-fetch": "^2.5.10",
    "@types/polka": "^0.5.2",
    "autoprefixer": "^10.2.6",
    "chai-sorted": "^0.2.0",
    "cypress": "^9.2.0",
    "cypress-pipe": "^2.0.0",
    "hasura-cli": "^2.0.9",
    "mdsvex": "^0.9.7",
    "node-wait-for-it": "^0.2.0",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.10.0",
    "postcss": "^8.3.5",
    "prettier": "^2.3.1",
    "prettier-plugin-svelte": "^2.3.1",
    "pretty-quick": "^3.1.1",
    "sass": "^1.35.1",
    "svelte-check": "^2.1.0",
    "svelte-preprocess": "^4.7.3",
    "tslib": "^2.3.0",
    "vite": "^2.7.6"
  },
  "engines": {
    "node": ">= 12.17.0"
  },
  "type": "module",
  "resolutions": {}
}
