{
  "name": "cucb-website",
  "description": "meow meow meow",
  "version": "0.0.1",
  "scripts": {
    "dev": "sapper dev -p 4000",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "cy:run:ci": "percy exec -- cypress run",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "format": "pretty-quick",
    "test": "run-p --race dev cy:run",
    "test:ci": "run-p --race dev cy:run:ci",
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose stop",
    "docker:down": "docker-compose down",
    "docker:test": "docker-compose -f docker-compose.yml -f docker-compose.test.yml up --exit-code-from cypress",
    "hasura:console": "cd hasura && hasura console --endpoint \"http://localhost:8080\" --admin-secret myadminsecretkey"
  },
  "dependencies": {
    "apollo-boost": "^0.4.7",
    "apollo-graphql": "^0.4.1",
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.1",
    "connect-pg-simple": "^6.1.0",
    "express-session": "^1.17.1",
    "graphql": "^0.13.2",
    "graphql-import": "^1.0.0",
    "graphql-tag": "^2.10.3",
    "graphql-tools": "^3.0.2",
    "hasura-cli": "^1.2.1",
    "http-proxy": "^1.18.0",
    "immutable": "^4.0.0-rc.12",
    "node": "^13.12.0",
    "pg": "^8.0.2",
    "polka": "next",
    "showdown": "^1.9.1",
    "sirv": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "autoprefixer": "^9.7.4",
    "cypress-plugin-retries": "^1.5.2",
    "husky": "^4.2.5",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "prettier-plugin-svelte": "^0.7.0",
    "pretty-quick": "^2.0.1",
    "rollup": "^1.20.0",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^4.0.4",
    "sapper": "^0.27.0",
    "svelte": "^3.0.0",
    "svelte-preprocess": "^3.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "docker-compose -f docker-compose.test.yml up --exit-code-from cypress"
    }
  }
}
